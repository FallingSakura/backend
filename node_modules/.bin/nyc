#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/nyc/bin/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/nyc/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules:/home/fallingsakura/CodeProgram/Back-End/node_modules:/home/fallingsakura/CodeProgram/node_modules:/home/fallingsakura/node_modules:/home/node_modules:/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/nyc/bin/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/nyc/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules:/home/fallingsakura/CodeProgram/Back-End/node_modules:/home/fallingsakura/CodeProgram/node_modules:/home/fallingsakura/node_modules:/home/node_modules:/node_modules:/home/fallingsakura/CodeProgram/Back-End/Functions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nyc/bin/nyc.js" "$@"
else
  exec node  "$basedir/../nyc/bin/nyc.js" "$@"
fi
